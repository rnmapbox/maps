    "debounce": "^2.2.0",
    "fbjs": "^3.0.5",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.2",
    "react-native-vector-icons": "^10.3.0",
