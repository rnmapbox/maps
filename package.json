    "eslint-plugin-prettier": "^5.5.4",
    "expo": "^54.0.10",
    "expo-module-scripts": "^5.0.7",
    "globals": "^16.4.0",
    "jest": "29.7.0",
    "jest-cli": "30.2.0",
    "mapbox-gl": "^3.15.0",
    "node-dir": "0.1.17",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-docgen": "rnmapbox/react-docgen#rnmapbox-dist-react-docgen-v6",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.40.13",
    "react-test-renderer": "19.1.0",
    "release-it": "^19.0.4",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@turf/along": "6.5.0",
    "@turf/distance": "6.5.0",
